web: cd src && python manage.py migrate && ([ ! -z "$DJANGO_SUPERUSER_PASSWORD" ] && python manage.py ensure_superuser || echo "Skipping superuser creation: DJANGO_SUPERUSER_PASSWORD not set") && python manage.py collectstatic --noinput && gunicorn myyoutubeprocessor.wsgi:application --log-file -
# worker: cd src && celery -A myyoutubeprocessor worker --loglevel=info